/*鼠标经过放大动画*/
.weight .left img, .hot .left img, .hot .min_hot li img, .trend .left .img1 img, .brand .right li.li1 img, .brand .right li.li2 img, .brand .right li.li3 img, .brand .right li.li4 img, .suit-dress li.li1 img, .suit-dress li.li2 img, .accessories .left li.li1 img, .accessories .left li.li2 img, .accessories .left li.li3 img, .accessories .center img, .beautiful li.li1 img, .beautiful li.li2 img { -webkit-transition: -webkit-transform .2s linear 0s; -moz-transition: -moz-transform .2s linear 0s; -ms-transition: -ms-transform .2s linear 0s; -o-transition: -o-transform .2s linear 0s; transition: transform .2s linear 0s; }
