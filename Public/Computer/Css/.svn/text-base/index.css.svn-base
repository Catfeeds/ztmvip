.hot .min_hot li img{ width: 272px; height: 245px;}
.hot .min_hot li.li3, .hot .min_hot li.li4 { margin-bottom: 0px; }
.hot .right { width: 171px; height: 498px; border: 1px solid #dddddd; float: left; margin-left: 10px; padding: 0px 27px 0px 25px; }
.hot .right .title { }
.hot .right li { line-height: 31px;/* font-size: 12px;*/ }
.hot .right li p { border-bottom: 1px dashed #dddddd; }
.hot .right li.li9 p { border-bottom: none; }
.hot .right li a { color: #252525; }
.hot .right li.hover a { color: #e9c574; }
.hot .right li img { display: none; margin-top: 9px; width: 171px; height: 130px; }
