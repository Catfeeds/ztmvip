/*品牌brand*/
.brand { width: 1200px; margin: auto; overflow: hidden; }
.brand .left { float: left; width: 223px; height: 498px; border: 1px solid #dddddd; margin-right: 10px; }
.brand .left li { border-top: 1px solid #dddddd; width: 111px; float: left; }
