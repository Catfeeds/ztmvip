.weight .right li .p2 span.span2 {/*font-size: 12px; */ color: #808080; margin: 0px; padding: 0px; position: relative; left: -9px; }
/*热点分享*/
.hot { width: 1200px; margin: 0 auto; clear: both; overflow: hidden; }
.hot .left { width: 400px; height: 500px; float: left; overflow: hidden; }
.hot .min_hot { overflow: hidden; width: 564px; height:500px; float: left; }
