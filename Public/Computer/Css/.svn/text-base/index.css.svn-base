.hot .right li.hover p { display: block; border-bottom: 1px dashed #dddddd; }
.hot .right li.title { padding: 27px 0px 7px 0px; }
.hot .right li.title span { display: block; width: 130px; height: 16px; margin: auto; background: url(index_0343.png); text-align: center; position: relative; font-size: 14px; }
.hot .right li.title span i { position: absolute; left: 37px; top: -8px; }
/*潮流趋势*/
.trend { width: 1200px; margin: auto; overflow: hidden; }
