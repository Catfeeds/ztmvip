/*mini轮播*/
.banner-container{ width:1197px; height:179px; border:1px solid #dddddd; margin: auto; overflow: hidden; position: relative; margin-top: 41px;}
.banner-container ul{ height:180px; position: relative; left: 0;top: 0;}
.banner-container ul li{ width: 169px;border-left:1px solid #fff; border-right:1px solid #fff ; float: left; position: relative;}
.banner-container ul li:hover{ border-color: #ddd;}
.banner-container ul li img{ width: 100%;}
.banner-container .left{display: none; width: 30px; height:68px; position: absolute;z-index: 1; top: 56px; left: 0;cursor:pointer;}
.banner-container .right{display: none; width: 30px; height:68px; position: absolute; z-index: 1; top: 56px; right: 0;cursor:pointer;}
@-webkit-keyframes banner_mini_left_donghua {
    0% { left:-30px; }
    100% { left:0px; }
}
@-moz-keyframes banner_mini_left_donghua {
    0% { left:-30px; }
    100% { left:0px; }
}
@-ms-keyframes banner_mini_left_donghua {
    0% { left:-30px; }
    100% { left:0px; }
}
@-o-keyframes banner_mini_left_donghua {
    0% { left:-30px; }
    100% { left:0px; }
}
@keyframes banner_mini_left_donghua {
    0% { left:-30px; }
    100% { left:0px; }
}
.banner-container:hover .left{ 
-webkit-animation: banner_mini_left_donghua .2s linear 0s 1 normal;
   -moz-animation: banner_mini_left_donghua .2s linear 0s 1 normal;
    -ms-animation: banner_mini_left_donghua .2s linear 0s 1 normal;
     -o-animation: banner_mini_left_donghua .2s linear 0s 1 normal;
        animation: banner_mini_left_donghua .2s linear 0s 1 normal;
    }
@-webkit-keyframes banner_mini_right_donghua {
    0% { right:-30px; }
    100% { right:0px; }
}
@-moz-keyframes banner_mini_right_donghua {
    0% { right:-30px; }
    100% { right:0px; }
}
@-ms-keyframes banner_mini_right_donghua {
    0% { right:-30px; }
    100% { right:0px; }
}
@-o-keyframes banner_mini_right_donghua {
    0% { right:-30px; }
    100% { right:0px; }
}
@keyframes banner_mini_right_donghua {
    0% { right:-30px; }
    100% { right:0px; }
}
.banner-container:hover .right{
    -webkit-animation: banner_mini_right_donghua .2s linear 0s 1 normal;
       -moz-animation: banner_mini_right_donghua .2s linear 0s 1 normal;
        -ms-animation: banner_mini_right_donghua .2s linear 0s 1 normal;
         -o-animation: banner_mini_right_donghua .2s linear 0s 1 normal;
            animation: banner_mini_right_donghua .2s linear 0s 1 normal;
}


.banner-container:hover .left,.banner-container:hover .right{display: inline-block;}
.banner-container-title{ position: relative; width:1200px; margin:auto;}
.banner-container-title span{ font-size: 20px; font-weight: 600; color:#fff; display:block; width:135px; height:30px; line-height: 30px; background:url(index_17.jpg); position: absolute; left:0px; top: -197px; text-indent: 20px;}
.banner-container ul li div{ cursor:pointer; position: absolute; width:140px; height:50px; padding:0px 15px; bottom:-50px; left:0px; background:url(index_0aaa3.png);
-webkit-transition: bottom .2s ease 0s;
   -moz-transition: bottom .2s ease 0s;
    -ms-transition: bottom .2s ease 0s;
     -o-transition: bottom .2s ease 0s;
        transition: bottom .2s ease 0s;
}

.banner-container ul li:hover div{
bottom:0px;}
.banner-container ul li div .p1{ font-size: 12px; color:#fff; text-align: center; padding-top: 8px; padding-bottom: 3px; overflow:hidden;
text-overflow:ellipsis;
white-space :nowrap;
}
.banner-container ul li div .p2{ font-size: 14px; color:#fff; text-align: center;}
.banner-container ul li div a{ color:#fff;}
.banner-container ul li div a:hover{ text-decoration: underline;}



