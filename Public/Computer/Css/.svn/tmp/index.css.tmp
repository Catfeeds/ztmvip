@charset "utf-8";
/* CSS Document */





/*圆圈导航*/
.index_nav3 { width:1200px; margin:auto; padding-top: 26px; overflow: hidden; font-family: "宋体";}
.index_nav3 li{ width:73px;  margin-left: 80px; margin-right: 34px; float:left; text-align: center; position: relative;}
.index_nav3 li img{ border-radius:50%;}

.index_nav3 li .hover{ display:block; width:67px; height:67px; position: absolute; top: 50%; left:50%;margin-left: -36.5px;margin-top: -59.5px;  border-radius: 50%; z-index: -1;
    border: 3px solid #fff;
-webkit-transform: scale(1.5);
   -moz-transform: scale(1.5);
    -ms-transform: scale(1.5);
     -o-transform: scale(1.5);
        transform: scale(1.5);

    -webkit-transition:-webkit-transform .2s ease-out 0s,border-color 0.2s ease-out 0s;
       -moz-transition:   -moz-transform .2s ease-out 0s,border-color 0.2s ease-out 0s;
        -ms-transition:    -ms-transform .2s ease-out 0s,border-color 0.2s ease-out 0s;
         -o-transition:     -o-transform .2s ease-out 0s,border-color 0.2s ease-out 0s;
            transition:        transform .2s ease-out 0s,border-color 0.2s ease-out 0s;
}
.index_nav3 li:hover .hover{ /*z-index: 1; */
-webkit-transform: scale(1.25);
   -moz-transform: scale(1.25);
    -ms-transform: scale(1.25);
     -o-transform: scale(1.25);
        transform: scale(1.25);
    }
.index_nav3 li.li1:hover .hover{ border-color: #ff8181;}
.index_nav3 li.li2:hover .hover{ border-color: #ffa868;}
.index_nav3 li.li3:hover .hover{ border-color: #ffd474;}
.index_nav3 li.li4:hover .hover{ border-color: #96ffa0;}
.index_nav3 li.li5:hover .hover{ border-color: #87f3fa;}
.index_nav3 li.li6:hover .hover{ border-color: #d488ff;}    

.index_nav3 li p a{color:#808080; font-size: 18px; position: relative; z-index: 2;}
.index_nav3 li p { padding-top: 26px; line-height: 18px;} 
.index_nav3 li:hover p a{ color:#252525;}
/*马赛克横向标题*/
.index_title_hr{ background:url(index_03.png); height:11px; width:1200px; margin:auto; margin-top: 80px; position: relative; margin-bottom: 26px; clear:both;}
.index_title_hr h1{ display: inline-block; width:240px; text-align: center; font-weight: 600; color:#252525; font-size: 24px; position: absolute; left:50%; top:-14px; margin-left: -120px;}
.index_title_hr_a{ width:1200px; margin:auto; padding-bottom: 20px; text-align: center;}
.index_title_hr_a a{ font-size: 15px; color:#252525; padding:0px 20px;}
.index_title_hr_a a:hover{text-decoration: underline;}

/*减重方案*/
.weight{ width:1200px; margin: auto; overflow: hidden;}
.weight .left{ width:755px; height:500px; float:left; overflow: hidden;}


.weight .right{ float:left; width:435px; margin-left:  10px;}
