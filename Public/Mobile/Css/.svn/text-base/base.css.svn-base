@charset "utf-8";
/* CSS Document */
body,nav,dl,dt,dd,p,h1,h2,h3,h4,ul,ol,li,input,button,textarea,footer{
    margin:0;
    padding:0;
}
u,em,i{
    font-style:normal;text-decoration: none;}
a{text-decoration: none;color:#5F5F5F;}
body{
    background:#fff;
    min-width:320px;
    max-width:750px;
    color:#252525;
    -webkit-tap-highlight-color: transparent;
}
html{
    font-family:"微软雅黑","Heiti SC","Lucida Sans Unicode","Myriad Pro","Hiragino Sans GB","Heiti SC",Verdana,simsun;
    font-size: 12px;
    -webkit-text-size-adjust:100%;   /*让谷歌浏览器可以设定字体大小*/
    -ms-text-size-adjust:100%;
    max-width:750px;
    min-width:320px;
    margin:auto;
}
button{border:none;}
/* 设置HTML5元素为块 */
article,aside,details,figcaption,figure,footer,header,menu,nav,section {
display:block;
}
/* 设置图片视频等自适应调整 */
img {
width: 100%;
display:block;font-size: 0;border:0;
height:auto;
width:auto\9; /* ie8 
*/}
.video embed,.video object,.video iframe {
width:100%;
height:auto;
}
h1,h2,h3,h4,h5,h6{
    font-size:100%;
}
form{
    display:inline;
}
ul,ol{
    list-style:none;
}
input,button,textarea{outline:none;border-radius: 0;}
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
    -webkit-appearance: none !important;
    margin: 0;
}