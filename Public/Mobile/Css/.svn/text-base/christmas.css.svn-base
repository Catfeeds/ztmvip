@font-face {font-family: 'xuehua';
    src: url('font_xuehua/iconfont.eot'); /* IE9*/
    src: url('font_xuehua/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('font_xuehua/iconfont.woff') format('woff'), /* chrome、firefox */
    url('font_xuehua/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('font_xuehua/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}
.xuehua{
    font-family:"xuehua" !important;
    font-size:16px;font-style:normal;
    color:#fff;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
  }

  @-webkit-keyframes xuehuadonghua {
    /*0% { top:0;  }*/
    100% { top:100%;transform: rotate(2000deg); -webkit-transform: rotate(2000deg); }
  }
  @keyframes xuehuadonghua {
    /*0% { top:0;  }*/
    100% { top:100%;transform: rotate(2000deg); -webkit-transform: rotate(2000deg); }
  }

 .xuehua{
     display: inline-block;     position: absolute; left:0px; top:-4%; z-index: 9; color:rgba(255, 255, 255, 0.7);
      
   } 
