.swiper-container ul li{ float:left; white-space:nowrap;}
.swiper-container ul li img{ padding-right: 1%; width:99%;}*/

.jiangpin,.fenxiang{position: fixed; height:100%; width:100%; top: 0; left: 0; background: rgba(0,0,0,0.75); z-index: 10;}
.jiangpin .hb{ width:100%; margin-top: 24%; position: relative; z-index: 11;}
.jiangpin .linqu{z-index: 12; display:block; width:4em; position: absolute; left: 50%; margin-left: -2em; top:73%; font-size: 26px; color:#face00; text-decoration: underline;}


.fenxiang .text{ width:74%; margin:22% auto auto; background:#ff2400; border-top-left-radius: 15px;border-bottom-right-radius: 15px; padding-bottom: 12px; position: relative;}
.fenxiang .text h1{ font-size: 12px; color:#ffea00; font-weight: 400; line-height: 1em; padding-top: 15px; padding-left: 15px;}
.fenxiang .text p{ color:#fff; padding: 7px 5px 5px 34px ; position: relative;}
.fenxiang .text p img{ width:16px; position: absolute; left: 9px; top: 5px; border-radius: 50%; border:2px solid #6c0d14;}
.fenxiang .jiantou{ width:17%; position: absolute; top: 0;right:13%;}
.fenxiang .text  .false{ width:26px; margin: 5px; position: absolute; top:-18px; right:-18px; border-radius: 50%; background:rgba(0,0,0,.73); z-index: 20;}



