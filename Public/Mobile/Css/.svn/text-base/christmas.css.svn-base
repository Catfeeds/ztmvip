@charset "utf-8";
/* CSS Document */

html,body{width:100%; min-height: 100%; background:#970006;}
body{ background: url(../Images/christmas_bg.jpg) no-repeat 100% 100%/100% 100%;}
header img.logo{width:56%; margin: auto;}
header .title{overflow: hidden; position: relative; padding-left: 82px; padding-right: 26px; }

header .title .left{  height:112px;position: absolute;left: 0; top: 0;}

header .title .left .touxiang{ width:62.5px; height:62.5px; border-radius:100%; border:4px solid #df554f; margin-left: 18px; margin-top:38px;}
header .title .left .mao{width:75px; height:69px; position: absolute;
top: 0; left:4px;}

header .title .right{ width:100%; height:95px; margin-top: 15px; float: left;  background:url(../Images/christmas_draw.png) 0 0/100% 100%;}
header .title .right p{ font-size: 11px; color: #fff; padding-left: 11.6%; padding-right: 8%; line-height: 20px; padding-top: 10px;}

.container{ width:87%; margin-left: 6.5%; position: relative;}
.container ul{ height:100%; width:100%; position: relative;}
.container ul li{ width:20%; position: absolute; background:#ff6262;  border-radius:7.8%;}


@-webkit-keyframes donghua {
    0% { background:rgba(0, 0, 0, 0.0); }
    100% {  }
}
@-moz-keyframes donghua {
    0% { background:rgba(0, 0, 0, 0.0); }
    100% {  }
}
@-ms-keyframes donghua {
    0% { background:rgba(0, 0, 0, 0.0); }
    100% {  }
}
@-o-keyframes donghua {
    0% { background:rgba(0, 0, 0, 0.0); }
    100% {  }
}
@keyframes donghua {
    0% { background:rgba(0, 0, 0, 0.0); }
    100% {  }
}
.container ul li.adcls{background:rgba(255, 255, 255, 0.9);

          animation: donghua 0.7s linear 0s infinite normal;
          -webkit-animation: donghua 0.7s linear 0s infinite normal;
          -moz-animation: donghua 0.7s linear 0s infinite normal;
          -o-animation: donghua 0.7s linear 0s infinite normal;

}

.container ul li.li1{left:2.3%;   top:3.25%;}
.container ul li.li2{left:27.34%; top:3.25%;}
.container ul li.li3{left:52.4%;  top:3.25%;}
.container ul li.li4{right:2.3%;  top:3.25%;}
.container ul li.li5{right:2.3%;  top:28.5%;}
.container ul li.li6{right:2.3%;  top:53.75%;}
.container ul li.li7{right:2.3%; bottom: 0px;}
.container ul li.li8{left:52.4%; right:2.3%; bottom: 0px;}
.container ul li.li9{left:27.34%;  bottom: 0px;}
.container ul li.li10{left:2.3%; bottom: 0px;}
.container ul li.li11{left:2.3%;  top:53.75%;}
.container ul li.li12{left:2.3%;  top:28.5%;}

.container .mingdan{ background:rgba(0, 0, 0, 0.67); position: absolute; right:-15px; bottom:14%; line-height:1.5em; font-size: 10px; color:#fff; padding:2px 0px 2px 13px; width:47%; height:4.5em; overflow: hidden; border-radius: 4px; }
.container .swiper-wrapper{ }
.container .mingdan p{overflow:hidden;
text-overflow:ellipsis;
white-space :nowrap;
}

.container .bt{ background:none; width:44.7%; position: absolute; top: 27.85%; left:27.35%;}
.container .bt p{ font-size: 30px; color:#fff; position: absolute; left:0px; width:100%; text-align: center; top:56.5%;}

.zhufn_but{background:#ffe400; height:23px; line-height: 23px; font-size: 16px; color:#c70903; border-radius:100px; text-align: center; margin: 6px auto;width:8em; }

.swiper-container{ overflow: hidden; padding-top: 11px; padding-bottom: 1px; background:rgba(255,255,255,0.2);}
.donghua_none{ -webkit-animation:none;
   -moz-animation:  none 0 ease 0 1 normal;
    -ms-animation:  none 0 ease 0 1 normal;
     -o-animation:  none 0 ease 0 1 normal;
        animation:  none 0 ease 0 1 normal;}
