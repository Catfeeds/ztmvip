html{font-size:12px;font-family: "冬青黑体简体中文";height:100%;}
body{background:#fff;height:100%;}
.header_box{width:100%;max-width:750px;text-align: center;position:fixed;z-index:999;}
.return{display:block;width:9px;position:absolute;left:10px;top:11px;}
.header_title{height:40px;background:#fff;font-size:1.4rem;line-height: 40px;}
.address_box{padding-top:40px;background:#CDCDCD;margin-bottom:4%;}
.rt_home{display:block;width:30px;position:absolute;right:6px;top:5px;}
.num{width:15px;height:15px;line-height:15px;border-radius: 50%;background:#E9C573;color:#fff;position:absolute;top:2px;left:5px;}
.bag_box{background:#fff;margin:1px 0 1%;padding:0 10px;}
.bag_title{font-size: 1.2rem;line-height:40px;position:relative;}
.bag_title i{color:#FF0000;}
.tick{width:40px;height:40px;position:absolute;right:0px;top:0px;background:url(../Images/fhx.gif) no-repeat center right;background-size: 35%;}
.bag_in{display:none;overflow:hidden;}
.bag_in .bag,.show{overflow:hidden;padding-bottom:10px;}
.show .photo{margin-right:10px;}
.show .photo:last-child{margin-right:0;}
.photo{width:36%;float:left;}
.bag_inf{width:64%;padding-left:10px;box-sizing: border-box;float:left;font-size: 1.1rem;}
.show .swiper-slide{width:36%;}
.place_box{margin-top:1px;}
.place_box em{color:#6C6C6C;}
.place_box em u{text-decoration: line-through;}
.bag_box:nth-last-child(){margin-bottom:0;}
.pane{width:100px;height:28px;line-height:28px;font-size:1.1rem;color:#FF0000;border: 1px solid #FF0000;text-align: center;box-sizing: border-box;}
.add_in{height:60px;}
.add_bt{width:40%;height:40px;font-size: 1.2rem;color:#fff;line-height:40px;text-align:center;background:#E9C573;margin:10px auto;}


.bag_inf .choose{line-height:30px;border-top: 1px solid #7F7F7F;border-bottom: 1px solid #7F7F7F;box-sizing: border-box;position:relative;}
.bag_inf .choose .span{width:8px;position:absolute;right:0;top:7px;}
.value{display:none;}
.value1,.value2{padding:0 15px;height:30px;color:#fff;text-align:center;margin-right:5px;line-height:30px;background:#E8C473;display:block;float:left;}
.bg{width:100%;height:100%;max-width:750px;background:rgba(0,0,0,0.5);position:fixed;top:0;z-index: 999;display:none;}
.bg_box{width:90%;left:50%;margin-left:-45%;padding:10px;background:#fff;position:absolute;box-sizing: border-box;top:50%;margin-top: -105px;}
.title{font-size: 1.2rem;font-weight: bold;line-height:30px;}
.color_box{border-bottom: 1px solid #CDCDCD;height:40px;overflow:hidden;}
.color_box label{padding:0 15px;height:30px;color:#fff;text-align:center;margin-right:5px;line-height:30px;background:#CDCDCD;display:block;float:left;}
.size{border-bottom: 1px solid #CDCDCD;height:40px;overflow:hidden;}
.size label{padding:0 15px;height:30px;color:#fff;text-align:center;margin-right:5px;line-height:30px;background:#CDCDCD;display:block;float:left;}
.rdo_cur{background:#E9C574!important;}
.bg_box input{display:none;}
.bt_box{height:40px;overflow:hidden;margin-top:10px;}
.bt_box .cancel{width:35%;color:#E8C473;font-size:1.2rem;text-align:center;line-height:30px;height:30px;border: 1px solid #E8C473;float:left;margin-left:10%;box-sizing: border-box;}
.bt_box .determine{width:35%;color:#fff;font-size:1.2rem;text-align:center;line-height:30px;height:30px;background:#E8C473;float:right;margin-right:10%;}
.value .name{line-height:30px;}
.goods_num{display:block;margin:0 auto 5px;overflow:hidden;padding:5px;}
.goods_num input::-webkit-inner-spin-button {
     -webkit-appearance: none;
}
.goods_num input{width:30px;display:inline-block;text-align:center;border-radius: 0;border:0;border-left:1px solid #CDCDCD;border-right:1px solid #CDCDCD;margin:0 5px;}
.goods_num button{font-size: 1.2rem;background:#fff;border:0;outline:none;}
.province{overflow:hidden;}
.province .pane{float:left;}
.province .goods_num{float:right;}

.copyright{height:67px;font-size:0.8rem;text-align: center;line-height: 14px;padding-top:12px;box-sizing: border-box;color:#6d6d6d;}
.top{width:50px;height:40px;position:fixed;bottom:90px;right:5%;display:none;z-index: 999;}
.other{height:50px;background:#fff;}
