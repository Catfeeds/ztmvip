.six .text_box .text2 em{color:#ff9000;font-weight: bold;}
.seven{background:url(../images/seven.jpg) no-repeat top center;background-size: cover;color:#695906;}
.seven .text_box1{width:65%;position:absolute;top:19%;left:5%;font-size: 1.3rem;line-height: 20px;}
.seven .text_box2{width:95%;position:absolute;top:71%;left:5%;font-size: 1.2rem;line-height: 20px;}
.eight{background:url(../images/eight.jpg) no-repeat top center;background-size: cover;}
