.nine{background:url(../images/nine.jpg);background-size: cover;}
.ten{background:url(../images/ten.jpg);background-size: cover;}
.ten .text_box1{width:80%;position:absolute;top:52%;font-size:1.5rem;left:50%;margin-left:-40%;text-align:justify;color:#523A1E;font-weight: bold;}
.ten .text_box1 i{color:#f36c09;}
.ten .text_box3{width:80%;position:absolute;top:80%;left:10%;font-size: 1.3rem;text-align:center;}
.sqbt{width:40%;padding:5px 8px;background:#ff9600;border-radius: 3px;color:#fff;font-size: 1.3rem;margin:0 auto;}
.ten .text_box3 .inf i{color:#ff9600;}

.bg2{width:100%;height:100%;background:rgba(0,0,0,0.6);background-size: 50%;position:fixed;top:0;left:0;z-index: 999;display:none;}
.bg2 .text_box2{width:80%;position:absolute;top:50%;left:10%;font-size: 1.2rem;background:#fff;padding:5px;}
.bg2 .text_box2 .title{font-size: 1.2rem;text-align: center;padding:5px 0;color:#ff9600;}
.bg2 .text_box2 input{font-size: 1.1rem;padding-left:10px;box-sizing: border-box;}
.bg2 .text_box2 .name{width:30%;}
.bg2 .text_box2 .phone{width:39%;}
.bg2 .text_box2 .place{width:81%;}
.music_bt{position:absolute;width:8%;top:20px;right:20px;z-index: 999;}
.zhuan{animation: zhuan 2s linear 0s infinite;}
.bt_box{margin-top:2px;}
.bt_box .inf{float:left;font-size: 1.0rem;}
.bt_box .inf i{color:#ff9600;}
.bt_box .bt{float:right;padding:2px 5px;background:#ff9600;border-radius: 3px;color:#fff;}
.bg{width:100%;height:100%;background:rgba(0,0,0,0.6) url(../images/bg.png) no-repeat top right;background-size: 50%;position:fixed;top:0;left:0;z-index: 9999;display:none;}
.bg_in{width:80%;height:100px;position:absolute;left:50%;margin-left:-40%;top:25%;}
@keyframes zhuan{
    0%{
        -webkit-transform: rotate(0deg);
           -moz-transform: rotate(0deg);
            -ms-transform: rotate(0deg);
             -o-transform: rotate(0deg);
                transform: rotate(0deg);
    }

    100%{
        -webkit-transform: rotate(360deg);
           -moz-transform: rotate(360deg);
            -ms-transform: rotate(360deg);
             -o-transform: rotate(360deg);
                transform: rotate(360deg);
    }
}
