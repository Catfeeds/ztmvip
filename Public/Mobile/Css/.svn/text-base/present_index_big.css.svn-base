html, body {
        position: relative;
        height: 100%;
        font-size: 12px;
        font-family: "微软雅黑";
        background:#fff;
    }
    .swiper-container {
        width: 100%;
        max-width: 640px;
        height: 100%;
    }
.footer{width:100%;max-width:640px;background:#000;height:10%;position:absolute;bottom:0;z-index: 999;display: table;}
.footer img{width:70%;margin:5% auto;}
.logo_box{width:14%;position:absolute;top:10px;left:20px;z-index: 999;}
.logo_box .logo{animation: logo 5s linear 0s infinite;}
@keyframes logo{
    0%{
        -webkit-transform: rotateY(0deg);
           -moz-transform: rotateY(0deg);
            -ms-transform: rotateY(0deg);
             -o-transform: rotateY(0deg);
                transform: rotateY(0deg);
    }
    50%{
        -webkit-transform: rotateY(360deg);
           -moz-transform: rotateY(360deg);
            -ms-transform: rotateY(360deg);
             -o-transform: rotateY(360deg);
                transform: rotateY(360deg);
    }
    100%{
        -webkit-transform: rotateY(360deg);
           -moz-transform: rotateY(360deg);
            -ms-transform: rotateY(360deg);
             -o-transform: rotateY(360deg);
                transform: rotateY(360deg);
    }
}
.one{background:url(../images/one.jpg) no-repeat top center;background-size: cover;}
.two{background:url(../images/two.jpg) no-repeat top center;background-size: cover;position:relative;}
.two .text{width:100%;position:absolute;top:50%;text-align: center;color: #402600;line-height: 20px;}
.two .text em{font-size: 1.3rem;}
.two .text i{line-height:30px;font-size: 1.4rem;}
.two .text i u{font-weight:bold;font-size:1.4rem;}
.three{background:url(../images/three.jpg) no-repeat top center;background-size: cover;position:relative;}
.three .text_box{width:100%;font-size: 1.2rem;line-height:23px;position:absolute;top:71%;text-align: center;color:#a05c00;line-height: 20px;}
