.six .text_box .text1{color:#3a6e7c;font-size: 1.3rem;text-align: center;line-height: 30px;}
.six .text_box .text1 i{font-size: 1.6rem;font-weight: bold;}
.six .text_box .text2{font-size: 1.1rem;text-align:justify ;}
