html{font-size:12px;font-family: "冬青黑体简体中文";background:#F3F3F3;}
body{background:#F3F3F3;}
.header_box{width:100%;max-width:750px;text-align: center;position:fixed;z-index:999;}
.return{display:block;width:9px;position:absolute;left:12px;top:11px;}
.header_title{height:40px;background:#fff;font-size:1.4rem;line-height: 40px;}
.sale_banner{width:100%;padding-top:40px;}
.sale_banner .banner{margin-bottom:1.5%;}
.goods_box{width:100%;}
.goods_real{overflow:hidden;margin-top:1.6%;}
.goods_real .goods{width:47.6%;background:#fff;margin:0 1.6% 1.6% 1.6%;float:left;}
.goods_real .goods:nth-child(even){margin-left:0;}
.goods_real .goods_photo{width:100%;}
.goods_real .price{height:28px;line-height: 28px;text-align: center;position:relative;padding-left: 2%;}
.goods_real .now_price{font-size: 1.3rem;font-weight: bold;color: #ECC35D;float:left;margin-right:5px;line-height: 26px; margin-left: 5px;}
.goods_real .now_price i{font-size:1.5rem;}
.goods_real .past_price{text-decoration: line-through;color:#AEAEAE;float:left; line-height: 1em; padding-top: 10px;}
.goods_real .mark{width:40px;height:13px;line-height:13px;border: 1px solid #FF4C91;color:#FF4C91;position:absolute;top:7px;right:5px; font-size: 11px;;}
.goods_real .inf{ color:#7F7F7F;line-height:15px;height:30px;padding-left:0.5em;padding-left: 2%;}
.loading{height:45px;width:70px;text-align: center;background:rgba(0,0,0,0.6);color:#fff;line-height: 45px;padding:0 10px;position:fixed;top:50%;margin-top:-22.5px;left:50%;margin-left:-35px;display:none;}

.copyright{height:67px;font-size:0.8rem;text-align: center;line-height: 14px;padding-top:12px;box-sizing: border-box;color:#6d6d6d;}
.top{width:40px;height:40px;position:fixed;bottom:10%;right:5%;display:none;}
.collect_click{width:29px;position:absolute; right: 2.5%; top: 5px}
.l_jiazai{width:40px;height:40px;margin:0 auto;
    animation: jiazai 2s linear 0s infinite;
    /* Firefox: */
    -moz-animation: jiazai 2s linear 0s infinite;
    /* Safari 和 Chrome: */
    -webkit-animation: jiazai 2s linear 0s infinite;
    /* Opera: */
    -o-animation: jiazai 2s linear 0s infinite;}
@keyframes jiazai{
    0%{
        -webkit-transform: rotate(0deg);
        -moz-transform: rotate(0deg);
        -ms-transform: rotate(0deg);
        -o-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -ms-transform: rotate(360deg);
        -o-transform: rotate(360deg);
        transform: rotate(360deg);
    }

}
@-moz-keyframes jiazai /* Firefox */
{
    0%{
        -webkit-transform: rotate(0deg);
        -moz-transform: rotate(0deg);
        -ms-transform: rotate(0deg);
        -o-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -ms-transform: rotate(360deg);
        -o-transform: rotate(360deg);
        transform: rotate(360deg);
    }
}

@-webkit-keyframes jiazai /* Safari 和 Chrome */
{
    0%{
        -webkit-transform: rotate(0deg);
        -moz-transform: rotate(0deg);
        -ms-transform: rotate(0deg);
        -o-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -ms-transform: rotate(360deg);
        -o-transform: rotate(360deg);
        transform: rotate(360deg);
    }
}

@-o-keyframes jiazai /* Opera */
{
    0%{
        -webkit-transform: rotate(0deg);
        -moz-transform: rotate(0deg);
        -ms-transform: rotate(0deg);
        -o-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -ms-transform: rotate(360deg);
        -o-transform: rotate(360deg);
        transform: rotate(360deg);
    }
}

