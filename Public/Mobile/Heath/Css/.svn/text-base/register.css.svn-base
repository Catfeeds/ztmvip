@charset "utf-8";
/* CSS Document */
.register{ padding:0px 20px;}

.register .contairen>li{ height:60px; line-height: 60px; border-bottom:1px solid #cecece; position: relative; padding-left: 88px; padding-right: 22px;}
.register .contairen > li  > span{ color: #252525; font-size: 14px; position: absolute;left: 1px; width:88px;}
.register .contairen > li  >input{ height:34px; line-height: 34px; border:0px; background: #f1f1f1; width:100%; font-size: 12px; color:#252525; text-indent: 20px;}
/*.register .contairen > li .input{ height:34px; line-height: 34px; border:0px; margin:auto; background: #f1f1f1; width:100%; position: relative; top: 13px;font-size: 12px; color:#252525;text-indent: 20px;}*/

@font-face {font-family: 'font_right';
    src: url('font_right/iconfont.eot'); /* IE9*/
    src: url('font_right/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('font_right/iconfont.woff') format('woff'), /* chrome、firefox */
    url('font_right/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('font_right/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}
.font_right{
    font-family:"font_right" !important;
    font-size:16px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;}
.register .contairen > li  >.font_right{ position: absolute;right:32px; top: 0; color:#949494;}
.register .contairen > li >i{position: absolute;right:35px; top: 0; color:#949494;}
.register .contairen > li.name{ height:70px; padding-top: 10px;}
.register .contairen > li.name p{  font-size: 11px; line-height: 21px; position: relative; top: -12px; padding-left: 7px; color:#808080;overflow:hidden;
text-overflow:ellipsis;
white-space :nowrap;
}
.register .contairen > li.sex{ text-align: center;}
.register .contairen > li.sex span{ text-align: left;}
.register .contairen > li.sex img{ width:60px; height:60px; display: inline-block;
vertical-align: top;
zoom: 1; /* Fix for IE7 */
*display: inline; /* Fix for IE7 */ margin:0px 10px;}

.register button{background:#e9c574; font-size: 18px; height:44px; line-height: 44px; width:100%; margin: 20px auto 10px; color:#fff; border:0;}

.bg_hui{ position: fixed; left:0; top: 0; height:100%; width:100%; background:rgba(0, 0, 0, 0.7); z-index:5 ;}






.date {position: fixed; z-index: 10; left:0; top: 0; right:0; bottom:0; margin:auto; width:250px; height:295px; background:#fff;}
.date p.p_top{ height:44px; text-align: center; line-height: 44px; font-size: 14px; color:#252525; border-bottom:1px solid #808080;}
.date p em{ font-size: 12px; display: inline-block; width:33%; text-align: center; line-height: 30px;}
.date .select{height:165px; width:33%; float:left; text-align: center; border-bottom:1px solid #808080; overflow: hidden; padding-bottom: 15px;}
.date .select .swiper-wrapper{ margin-top: 68px;}
.date .select .swiper-slide{ line-height: 34px; color:#d6d6d6;}
.date .select .swiper-slide-prev,.date .select .swiper-slide-next{ color:#7f7f7f;}
.date .select .swiper-slide-active{ color:#252525;}
.date span{ height:39px; line-height: 39px; text-align: center; width:49%; display: inline-block; color:#252525;}
.date span:first-of-type { border-right: 1px solid #808080;}

.date .xian_top,.date .xian_top1,.date .xian_top2{background:url(../Images/tm_03.png) 0 0 /100% 85px; width:40px; height:25px; position: absolute; top: 75px; left: 22px;}
.date .xian_bot,.date .xian_bot1,.date .xian_bot2{-webkit-transform: rotate(180deg);
   -moz-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
     -o-transform: rotate(180deg);
        transform: rotate(180deg); top: 58px;}
.date .xian_top1{ left: 103px;}
.date .xian_top2{ left: 185px;;}





