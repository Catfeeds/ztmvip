 .js_zq {position: fixed; z-index: 30; left:0; top: 0; right:0; bottom:0; margin:auto; width:150px; height:306px; background:#fff;}
 .js_zq p{ height:44px; text-align: center; line-height: 44px; font-size: 14px; color:#252525;}
 .js_zq .select{ height:220px; width:100%; text-align: center; border:1px solid #808080; border-width: 1px 0px; overflow: hidden;}
 .js_zq .select .swiper-wrapper{ margin-top: 88px;}
 .js_zq .select .swiper-slide{ height:44px; line-height:44px; color:#d6d6d6;
    }
 .js_zq .select .swiper-slide-prev, .js_zq .select .swiper-slide-next{ color:#7f7f7f;}
 .js_zq .select .swiper-slide-active{ color:#252525;}
 .js_zq span{ height:39px; line-height: 39px; text-align: center; width:49%; display: inline-block; color:#252525;}
 .js_zq span:first-of-type { border-right: 1px solid #808080;}
 .js_zq .xian_top{background:url(../Images/tm_03.png) 0 0 /100% 85px; width:40px; height:25px; position: absolute; top: 45px; left: 53px;}
.js_zq .xian_bot{-webkit-transform: rotate(180deg);
   -moz-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
     -o-transform: rotate(180deg);
        transform: rotate(180deg); top:240px;}

.swiper-container{ background:url("../Images/huang.jpg") no-repeat scroll 22px 67px / 39px 1px,url("../Images/huang.jpg") no-repeat scroll 22px  101px / 39px 1px;}
.js_zq .swiper-container{ background:url("../Images/huang.jpg") no-repeat scroll 53px 90px / 39px 1px,url("../Images/huang.jpg") no-repeat scroll 53px  128px / 39px 1px;}



.tishi{ width:304px; min-height:216px; background:#fff; position: fixed; top: 50%; margin-top: -108px; left: 50%; margin-left: -152px; z-index: 40;}

@font-face {font-family: 'tishi_font';
    src: url('tishi_font/iconfont.eot'); /* IE9*/
    src: url('tishi_font/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('tishi_font/iconfont.woff') format('woff'), /* chrome、firefox */
    url('tishi_font/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('tishi_font/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}

.tishi_font{
    font-family:"tishi_font" !important;
    font-size:35px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale; color:#fff;
    position: relative;top:5px;
}
.tishi .top{
    background:#e9c574; border-radius: 50%;
     width:49px; height:49px; text-height: 49px; text-align: center; position: absolute;top:-25px; left:126px;

}
.tishi h1{font-size: 18px; color:#252525;font-weight: 400; text-align: center; padding-top: 45px; padding-bottom:15px;}
.tishi p{ display:block; width:220px; text-align: center; font-size: 16px; color:#252525; border-top: 1px solid #252525; margin:auto; min-height:40px; padding-top: 10px; padding-bottom: 5px;}
.tishi span{ display:block;width:100px; height:34px; line-height: 34px; background:#e9c574; color:#fff; font-size: 14px; text-align: center; margin:auto; margin-bottom: 20px;}



.register .result{ text-indent: 10px; height:50px; line-height: 50px; font-size:14px; color:#252525;}
.register .result a{color:#252525; padding-left: 10px; text-decoration: underline;}



