/*20151231*/
.border_b{ border-bottom: none !important;}
.character{ height:100px; border:1px solid #e3e3e4; border-width: 1px 0px;}
.character ul{ height:100px;}
.character li{  float: left; padding-left: 10px; padding-top: 10px;  padding-bottom: 10px;}
.character li .img{height:78px; width:78px; float: left; border-radius:100px; overflow: hidden; border:2px solid #dddddd; margin-right: 10px;}
.character li .p1{ font-size: 11px; color:#252525; line-height: 22px; height:22px;}
.character li .p2{ font-size: 10px; line-height: 12px; height:14px; padding-bottom: 2px; padding-top: 2px;overflow:hidden;
text-overflow:ellipsis;
white-space :nowrap;
} 
.character li .p3{  font-size: 10px; line-height: 13px; height:33px;}