html{font-size:12px;font-family: "冬青黑体简体中文";height:100%;}
body{height:100%;background:#F1F1F1;}
.head_box{width:100%;max-width: 750px;position:fixed;top:0;background:#fff;z-index: 999;}
.head{width:100%;height:44px;border-bottom: 1px solid #ccc;text-align: center;font-size: 1.4rem;line-height:44px;position:relative;}
.head .return{width:9px;position:absolute;left:10px;top:13px;}
.columns_box{width:100%;height:44px;border-bottom: 1px solid #ccc;}
.columns_in{width:50%;height:44px;float:left;text-align: center;box-sizing: border-box;}
.columns_in .columns{width:100%;font-size: 1.1rem;height:17px;line-height:17px;margin-top:13px;border-right: 1px solid #ccc;color:#7F7F7F;box-sizing: border-box;}
.cur{border-bottom: 2px solid #E8C473;}
.cur .columns{color:#252525;}
article{padding-top:90px;}
.list_box{min-height:80%;display: none;text-align: center;font-size: 1.4rem;color:#7F7F7F;line-height:400px;}
.accord{display:block;}
.list{overflow: hidden;border-bottom: 1px solid #7F7F7F;line-height:55px;height:55px;background:#fff;}
.list:last-child{border:0;}
.list .what{float:left;font-size: 1.2rem;color:#252525;margin-left: 10px;}
.list .time{float:right;font-size: 1.0rem;color:#7F7F7F;margin-right: 10px;}
.bt_box{width:100%;max-width:750px;position:fixed;bottom:0;padding:10px 0;text-align: center;}
.bt{width:90%;height:44px;background:#E9C574;line-height:44px;font-size: 1.3rem;margin:0 auto;color:#fff;}
.two{background:#fff;font-size: 1.1rem;}
.two .bt1{width:37%;height:34px;line-height:34px;float:left;margin-left:10%;border: 1px solid #E9C574;color:#E9C574;box-sizing: border-box;}
.two .bt2{width:37%;height:34px;line-height:34px;float:right;margin-right:10%;background:#E9C574;color:#fff;}
.placeholder{height:54px;}

.bg{width:100%;height:100%;background:rgba(0,0,0,0.6);position:fixed;top:0;z-index: 999;display:none;}
.remind{width:194px;height:58px;background:#fff;position:absolute;top:50%;margin-top:-29px;left:50%;margin-left:-97px;text-align: center;line-height: 58px;font-size: 1.3rem;}
.bg .remind_box{width:90%;height:150px;top:50%;margin-top:-75px;font-size: 1.2rem;text-align: center;background:#fff;position:absolute;left:50%;margin-left:-45%;}
.remind_box .text{line-height: 54px;margin-top: 20px;}
.remindbt_box .remind_bt1{width:125px;height:40px;line-height:40px;float:left;border: 1px solid #E9C574;box-sizing: border-box;margin-left: 6%;color:#E9C574;}
.remindbt_box .remind_bt2{width:125px;height:40px;line-height: 40px;float:right;background:#E9C574;box-sizing: border-box;margin-right: 6%;color:#fff;}
.bg3 .remind_box{height:160px;margin-top:-80px;padding-top:20px;}
.price_num{border:0;border-bottom: 1px solid #252525;width:74px;text-align: center;}
.bg3 .remindbt_box{position:absolute;top:120px;width:100%;}
.text_box{width:100%;position:absolute;top:30px;z-index: 20;}
.text_password{width:262px;position:absolute;left:50%;margin-left:-131px;top:82px;text-align: left;line-height: 26px;}
.text_password i{display:inline-block;width:144px;height:100%;position:absolute;background:url(../Images/password_bg.jpg) no-repeat center bottom;background-size: contain;}
.text_password i input{width:144px;height:60px;border:0;font-size: 5rem;position:absolute;left:1px;top:-20px;background:none;letter-spacing:3px;}
.bg4 .remind_box{height:160px;margin-top:-80px;padding-top:20px;}
.bg4 .remindbt_box{position:absolute;top:120px;width:100%;}