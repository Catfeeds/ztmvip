@charset "utf-8";
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6,pre, form, fieldset, input, textarea, p, blockquote, th, td {padding: 0; margin: 0;}    
fieldset, img { border: 0; }    
table {border-collapse: collapse;  border-spacing: 0;}    
ol, ul {list-style: none; }    
address, caption, cite, code, dfn, em, strong, th, var { font-weight: normal; font-style: normal;}    
caption, th {text-align: left;}    
h1, h2, h3, h4, h5, h6 {font-weight: normal;font-size: 100%;}    
q:before, q:after {content: '';}    
abbr, acronym {border: 0;}
a{ text-decoration:none;}
input:focus,button:focus,select:focus{outline:none;}
.clear{ clear: both;}
html { font-size:10px; font-family:"冬青黑体简体中文";}


.max{ width:100%; max-width:750px; min-width:320px; margin:auto; overflow:hidden;}
.classify{ background:#f3f3f3;}
.classify .position{ overflow:hidden; position: fixed;width:100%; max-width:750px; min-width:320px; background:#fff;}

.classify .Search{ height:88px; background:#fff; padding-left:77px; padding-right:90px; position:relative;}
.classify .Search img{ position:absolute; left:20px; top:26px; width:18px; height:35px;}
.classify .Search input{ width:100%; border-width:0px; border-bottom:1px solid #808080; height:52px; line-height:52px; margin-top:18px; font-size:3.2rem;color:#8e8e8e; }
.classify .Search button{ position:absolute; right:20px; top:20px; width:50px; height:50px; background: url(../Images/classify_03.png) 0px 0px/ 100% 100% no-repeat;}



.classify .nav{ overflow:hidden;padding: 0px 11px; background:#f3f3f3;}
.classify .nav li{ float:left; width:25%; text-align:center; font-size:2.8rem; height:48px; margin-bottom:20px; padding-top:40px; border-bottom:10px solid #e9c574; color:#808080;}
.classify .nav .click {  border-bottom:10px solid #808080; color:#252525;}
.classify .con1 { padding-top: 202px;}
.classify .con1 li{ float:left; margin-left:1.33%; width:48%; background:#fff; margin-bottom:1.33%;}
.classify .con1 li img{ width:100%;}
.classify .con1 li p{ padding-left:10px; padding-right:10px;}
.classify .con1 li p.p1{ color:#ecc35d; font-size:3rem; padding-top:1rem; padding-bottom:5px;}
.classify .con1 li p.p2{ color:#808080; font-size:2.4rem; height:6rem;    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}


@font-face {font-family: "iconfont";
  src: url('myCustomer-iconfont/iconfont.eot'); /* IE9*/
  src: url('myCustomer-iconfont/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('myCustomer-iconfont/iconfont.woff') format('woff'), /* chrome、firefox */
  url('myCustomer-iconfont/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('myCustomer-iconfont/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.icon-jiagepaixu:before { content: "\e6e7"; }
