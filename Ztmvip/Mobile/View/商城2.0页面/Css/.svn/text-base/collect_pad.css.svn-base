html{font-size:24px;font-family: "冬青黑体简体中文";height:100%;}
body{height:100%;}
.head_box{width:100%;max-width: 750px;position:fixed;top:0;background:#fff;z-index: 999;}
.head{width:100%;height:88px;border-bottom: 1px solid #ccc;text-align: center;font-size: 1.4rem;line-height:88px;position:relative;}
.head .return{width:18px;position:absolute;left:20px;top:26px;}
.head .prompt{width:40px;height:40px;position:absolute;top:26px;right:20px;}
.columns_box{width:100%;height:88px;border-bottom: 1px solid #ccc;}
.columns_in{width:50%;height:88px;float:left;text-align: center;box-sizing: border-box;}
.columns_in .columns{width:100%;font-size: 1.1rem;height:34px;line-height:34px;margin-top:26px;border-right: 1px solid #ccc;color:#7F7F7F;box-sizing: border-box;}
.columns_in:last-child .columns{border:0;}
.cur{border-bottom: 4px solid #E8C473;}
.cur .columns{color:#252525;}
.acc{display:block!important;}

article{padding-top:180px;min-height:85%;}
.collect_box{display:none;overflow:hidden;}
.copyright{height:154px;text-align: center;line-height: 40px;padding-top:20px;box-sizing: border-box;}
.goods_in{width:100%;height:330px;padding:40px 0;overflow: hidden;position:relative;border-bottom: 1px solid #000;box-sizing: border-box;}
.goods_photo{width:250px;height:250px;border: 1px solid #000;overflow:hidden;position:absolute;left:20px;top:40px;}
.goods_inf{width:100%;padding-left: 290px;box-sizing: border-box;}
.goods_inf .title{font-size: 1.1rem;}
.goods_inf .price{color:#E8C473;line-height:30px;margin:40px 0;}
.goods_inf .price i{font-size: 1.4rem;font-weight: bold;}
.goods_inf .delate_bt{width:128px;height:58px;line-height:58px;text-align: center;font-size: 1.2rem;color:#ECC35D;border: 1px solid #ECC35D;}
.text_box{width:100%;height:380px;padding:40px 0;overflow: hidden;position:relative;border-bottom: 1px solid #000;box-sizing: border-box;}
.text_photo{width:200px;height:300px;overflow:hidden;position:absolute;left:20px;top:40px;}
.text_photo .text_name{position:absolute;width:100%;height:88px;background:rgba(0,0,0,0.6);bottom:10px;text-align: center;color:#fff;padding:10px 0;box-sizing: border-box;}
.text_inf{width:100%;height:300px;padding-left: 240px;box-sizing: border-box;}
.text_inf .title{font-size: 1.1rem;}
.text_inf .price{color:#E8C473;line-height:30px;margin:40px 0;}
.text_inf .price i{font-size: 1.4rem;font-weight: bold;}
.text_inf .delate_bt{width:128px;height:58px;line-height:58px;text-align: center;font-size: 1.2rem;color:#ECC35D;border: 1px solid #ECC35D;position:absolute;right:0;bottom:40px;}
.article{padding-top:89px;}