html{font-size:24px;font-family: "冬青黑体简体中文";height:100%;background:#F1F1F1;}
body{height:100%;background:#fff;}
.head_box{width:100%;max-width: 750px;position:fixed;top:0;background:#fff;z-index: 999;border: 1px solid #ccc;}
.head{width:100%;height:88px;text-align: center;font-size: 1.4rem;line-height:88px;position:relative;}
.head .return{width:18px;position:absolute;left:20px;top:26px;}
article{padding-top:88px;}
.rebate_box{margin:2% 0;height:356px;background:#fff;}
.rebate_inf{width:50%;height:198px;float: left;box-sizing: border-box;line-height:60px;text-align: center;font-size: 1.2rem;padding-top:28px;}
.rebate_inf:first-child{border-bottom:1px solid #F1F1F1;border-right:1px solid #F1F1F1;}
.rebate_inf:nth-child(2){border-bottom:1px solid #F1F1F1;}
.rebate_inf:nth-child(3){border-right:1px solid #F1F1F1;}
.list_box{padding:0 40px;box-sizing: border-box;background:#fff;font-size: 1.2rem;}
.list{border-bottom: 1px solid #CDCDCD;line-height: 90px;position:relative;}
.list i{color:#7F7F7F;font-size: 1rem;}
.list:last-child{border:0;}
.list .jump{width:16px;position:absolute;right:0;top:30px;}

.bt_box{width:100%;max-width:750px;position:fixed;bottom:0;padding:20px 0;text-align: center;background:#fff;font-size: 1.2rem;}
.bt_box .bt1{width:278px;height:68px;line-height: 68px;float:left;border: 1px solid #E9C574;box-sizing: border-box;margin-left: 6%;color:#E9C574;}
.bt_box .bt2{width:278px;height:68px;line-height: 68px;float:right;background:#E9C574;box-sizing: border-box;margin-right: 6%;color:#fff;}
.placeholder{height:136px;}

.bg{width:100%;max-width:750px;height:100%;background:rgba(0,0,0,0.6);position:fixed;top:0;z-index: 999;display:none;}
.remind{width:388px;height:116px;background:#fff;position:absolute;top:50%;margin-top:-58px;left:50%;margin-left:-194px;text-align: center;line-height: 116px;font-size: 1.3rem;}
.bg .remind_box{width:90%;height:300px;top:50%;margin-top:-150px;font-size: 1.2rem;text-align: center;background:#fff;position:absolute;left:50%;margin-left:-45%;}
.remind_box .text{line-height: 108px;margin-top: 40px;}
.bg4 .remind_box .text{line-height: 108px;margin:0 0 40px;}
.remindbt_box .remind_bt1{width:250px;height:80px;line-height:80px;float:left;border: 1px solid #E9C574;box-sizing: border-box;margin-left: 6%;color:#E9C574;}
.remindbt_box .remind_bt2{width:250px;height:80px;line-height: 80px;float:right;background:#E9C574;box-sizing: border-box;margin-right: 6%;color:#fff;}
.bg3 input{border:0;border-bottom: 1px solid #252525;width:148px;text-align: center;}
.bg4 .text{line-height: 108px;height:108px;}
.bg4 .text i{display:inline-block;width:288px;height:100%;position:relative;background:url(../Images/password_bg.jpg) no-repeat center bottom;background-size: contain;}
.bg4 .text input{width:288px;height:120px;border:0;font-size: 5rem;position:absolute;left:2px;top:16px;background:none;letter-spacing:6px;}