html{font-size:12px;font-family: "冬青黑体简体中文";}
.header_box{width:100%;max-width:750px;text-align: center;position:fixed;z-index: 999;}
.header_title{height:40px;background:#F3F3F3;font-size:1.4rem;line-height: 40px;}
.seckill_table{width:25%;height:40px;background:#252525;color: #fff;padding:8px;box-sizing: border-box;float: left;}
.seckill_table div{line-height: 100%;}
.seckill_table_box .current{background:#E9C574;}
.seckill_box{width:100%;overflow:hidden;padding-top:80px;}
.seckill_box .seckill_prompt{width:100%;height:40px;background:#F3F3F3;line-height: 40px;;overflow: hidden;}
.seckill_box .seckill_prompt .left{float:left;margin-left: 5px;}
.seckill_box .seckill_prompt .right{float:right;margin-right:5px;}
.seckill_box .seckill_prompt .right .colockbox span{display: block;float:left;}
.seckill_box .seckill_prompt .right .colockbox .symbol{margin:0 2px;}
.seckill_box .seckill{width:100%;display:none;}
.seckill_box .seckill .goods_box{width:100%;padding:2% 5px;box-sizing: border-box;font-size:1.1rem;border-bottom: 1px solid #808080;overflow:hidden;position:relative;}
.seckill_box .seckill .goods_box .goods_pho{width:33%;min-height:124px;float:left;}
.seckill_box .seckill .goods_box .goods_inf_box{width:67%;float:left;}
.seckill_box .seckill .goods_box .goods_inf{line-height:100%;}
.seckill_box .seckill .goods_box .ori_price{color:#808080;}
.seckill_box .seckill .goods_box .ori_price i{text-decoration: line-through;}
.seckill_box .seckill .goods_box .price{font-weight: bold;}
.seckill_box .seckill .goods_box .price i{font-size: 1.6rem;}
.seckill_bt{width:84px;height:30px;background:#C8C8C8;font-size: 1.4rem;color:#fff;text-align: center;line-height: 30px;position:absolute;bottom:5%;right:5px;}
.can{background:#ECC35D;}
.copyright{text-align: center;margin:2% 0;color:#6d6d6d;}
.top{width:40px;height:40px;position:fixed;bottom:10%;right:5%;display:none;z-index: 999;}
.other{height:50px;background:#fff;}
.nav_box{width:100%;max-width:750px;position:fixed;bottom:0;background:#252525;height:50px;z-index: 999;}
.nav_box .nav_in{width:25%;float:left;position:relative;}
.nav_box .nav{width:33px;margin:8.5px auto;}
.nav_box .goods_num{width:17px;height:17px;border-radius: 50%;background:#b8a070;color:#fff;text-align: center;line-height:17px;position:absolute;top:5px;left:50%;margin-left: -11.5px;}
.prompt_box{width:100%;height:100%;background:rgba(0,0,0,0.5);max-width: 750px;position:fixed;top:0;left:0;z-index: 9999;display:none;}
.prompt_in{width:90%;height:25%;min-height:170px;text-align: center;padding-top: 15px;box-sizing: border-box;background:#fff;margin:100px auto;}
.prompt_in .prompt_top{line-height:30px;font-size: 1.1rem;border-bottom: 1px solid #000;padding-bottom:10px;}
.prompt_top h3{font-size: 1.4rem;}
.prompt_bt{width:248px;height:40px;background:#E9C574;color:#fff;font-size: 1.4rem;line-height: 40px;margin:5% auto;}