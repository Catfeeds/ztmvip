html{font-size:12px;font-family: "冬青黑体简体中文";height:100%;}
body{min-height:100%;background:#fff;}
.head_box{width:100%;max-width: 750px;position:fixed;top:0;background:#fff;z-index: 999;border-bottom: 1px solid #ccc;}
.head{width:100%;height:44px;text-align: center;font-size: 1.4rem;line-height:44px;position:relative;}
.head .return{width:9px;position:absolute;left:10px;top:13px;}
article{padding-top:44px;}
.goods_box{width:100%;overflow:hidden;overflow:hidden;background:#fff;margin-top:2%;padding:0 10px;box-sizing: border-box;border-bottom: 1px solid #ccc;}
.goods_in{width:100%;padding:20px 0;box-sizing: border-box;overflow: hidden;}
.goods_photo{width:33%;float:left;}
.goods_inf{width:65%;float:right;}
.goods_inf .title{font-size: 1.1rem;}
.goods_inf .price{color:#E8C473;line-height:30px;margin-top:10px;}
.goods_inf .price i{font-size: 1.4rem;font-weight: bold;}
.goods_inf .price .left{float:right;color:#7F7F7F;}
.goods_inf .inf_other{color:#7F7F7F;}
.goods_inf .other_box{color:#7F7F7F;line-height: 40px;}
.inf_box{padding:0 10px;box-sizing: border-box;}
.score_box{overflow:hidden;height:30px;line-height:30px;margin-top:10px;font-size: 1.3rem;color:#B5B5B5;}
.score_box .name,.score_in{float:left;}
.score{float:left;width:24px;height:24px;margin:3px 10px 0 0;background:url(../Images/star2.jpg) no-repeat;background-size: cover;}
.cur{background:url(../Images/star.jpg) no-repeat;background-size: cover;}
.inf_box .title{height:30px;line-height:30px;font-size: 1.3rem;color:#B5B5B5;}
.text_box{margin-top:5px;}
.text{width:100%;height:150px;font-size:1.0rem;}
.text_inf{text-align: right;color:#B5B5B5;}
.bt_box{width:100%;max-width:750px;position:fixed;bottom:0;padding:10px 0;text-align: center;}
.bt{width:90%;height:44px;background:#E9C574;line-height:44px;font-size: 1.3rem;margin:0 auto;color:#fff;}
.placeholder{height:68px;}

.bg{width:100%;height:100%;background:rgba(0,0,0,0.6);position:fixed;top:0;z-index: 999;display:none;}
.remind{width:194px;height:58px;background:#fff;position:absolute;top:50%;margin-top:-29px;left:50%;margin-left:-97px;text-align: center;line-height: 58px;font-size: 1.3rem;}
.tick_box{width:40px;height:45px;position:absolute;left:0;top:0;}
.tick_box .rud{width:15px;height:15px;border: 1px solid #000;border-radius: 50%;position:absolute;top:10px;left:7px;}
.tick_box .tick{width:16px;height:16px;position:absolute;top:10px;left:7px;display:none;}

.input_bg{padding:2% 0;width:100%;background:#F1F1F1;}
.input_box{background:#fff;}
.input_box .title{padding:0 10px;box-sizing: border-box;font-size: 1.2rem;line-height:35px;}
.input_box .input{padding:0 40px;box-sizing: border-box;border-top: 1px solid #D3D3D3;line-height:45px;height:45px;font-size: 1.2rem;position:relative;}
.input input{display:none;}
.bgg{width:35%;margin:72px auto 30px;}
.bgg_inf{text-align: center;}
.bgg_inf .big{font-size: 1.4rem;}
.cent_box{height:75px;font-size: 1.1rem;padding:0 10px;border-bottom: 1px solid #7F7F7F;overflow:hidden;background:#fff;}
.cent_box .top{overflow:hidden;line-height: 25px;margin-top:15px;}
.cent_box .top .left{float:left;}
.cent_box .top .right{float:right;color:#7F7F7F;}


.saleafter_box{background:#fff;padding:0 10px;text-align:justify;line-height: 20px;font-size: 1.1rem;}
.saleafter_box h3{line-height: 35px;margin-top:8px;font-size: 1.2rem;font-weight: bold;}
